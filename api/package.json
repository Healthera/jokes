      "git add"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "1.18.3",
    "cors": "2.8.4",
    "express": "4.16.3",
    "lint-staged": "^7.3.0",
    "mongoose": "^5.4.14",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "eslint": "^5.6.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.1.2",
    "mocha": "^5.2.0",
    "prettier": "^1.14.3"
  }
}
