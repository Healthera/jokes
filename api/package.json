{
  "name": "api-jokes-healthera",
  "version": "0.0.1",
  "description": "REST API for Healthera's Jokes challenge",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha",
    "start": "node /app/index.js",
    "eslint": "./node_modules/.bin/eslint --fix ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/eslint --fix",
      "git add"
